<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:elevation="4dp">

    <ImageButton
        android:id="@+id/imageView2"
        android:layout_width="match_parent"
        android:layout_height="66dp"
        android:background="@drawable/rectangle_toolbar" />


    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="51dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="249dp"
        android:layout_marginEnd="252dp"
        android:layout_marginBottom="782dp"
        android:background="@drawable/circle_container" />


    <ImageView
        android:id="@+id/toolbar_whitelogo"
        android:layout_width="131dp"
        android:layout_height="93dp"
        android:layout_alignEnd="@+id/imageView2"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="231dp"
        android:layout_marginEnd="237dp"
        android:layout_marginBottom="785dp"
        app:srcCompat="@drawable/no_tenerife_whitelogo" />

    <ImageButton
        android:id="@+id/white_hamburgerIcon"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignEnd="@+id/imageView2"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="535dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/ic_menu_white_24dp" />

    <ImageButton
        android:id="@+id/white_searchLogo"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignEnd="@+id/imageView2"
        android:layout_alignParentTop="true"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="82dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/ic_search_white_24dp" />

    <ImageButton
        android:id="@+id/back_button"
        android:layout_width="80dp"
        android:layout_height="57dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="2dp"
        android:layout_marginTop="5dp"
        android:background="@drawable/ic_chevron_left_black_24dp" />


</RelativeLayout>