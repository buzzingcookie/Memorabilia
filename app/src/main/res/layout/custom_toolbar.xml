<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:elevation="4dp">

    <ImageButton
        android:id="@+id/imageView2"
        android:layout_width="match_parent"
        android:layout_height="66dp"
        android:background="@drawable/rectangle_toolbar" />


    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="90dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="259dp"
        android:layout_marginEnd="246dp"
        android:layout_marginBottom="794dp"
        android:background="@drawable/circle_container" />

    <ImageView
        android:id="@+id/toolbar_whitelogo"
        android:layout_width="135dp"
        android:layout_height="93dp"
        android:layout_alignEnd="@+id/imageView2"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="238dp"
        android:layout_marginEnd="227dp"
        android:layout_marginBottom="794dp"

        app:srcCompat="@drawable/no_tenerife_whitelogo" />

    <ImageButton
        android:id="@+id/white_hamburgerIcon"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignEnd="@+id/imageView2"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="540dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/ic_menu_white_24dp" />

    <ImageButton
        android:id="@+id/white_searchLogo"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignEnd="@+id/imageView2"
        android:layout_alignParentTop="true"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="78dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/ic_search_white_24dp" />


</RelativeLayout>